    "version": "0.2.0",
    "configurations": [
        
        {
            "command": "npm start",
            "name": "Run npm start",
            "request": "launch",
            "type": "node-terminal"
        },<script>
        LottieInteractivity.create({
          mode: 'scroll',
          player: '#firstLottie',
          actions: [
              {
                visibility: [0,1],
                type: 'seek',
                frames: [0, 390],
              },
            ],
        });
        </script>
        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        }
    ]
}